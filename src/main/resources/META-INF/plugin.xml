<idea-plugin>
    <id>org.jetbrains.plugins.compose.template</id>
    <name>Compose Template Example</name>
    <vendor url="https://www.yourcompany.com">YourCompany</vendor>

    <depends>com.intellij.modules.compose</depends>

    <resource-bundle>messages.ComposeTemplateBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.costafot.peepoplugin.toolWindow.ComposeSamplesToolWindowFactory"
                    icon="com.github.costafot.peepoplugin.ComposeIcons.ComposeToolWindow"
                    id="ComposeSamples"/>
    </extensions>
</idea-plugin>
